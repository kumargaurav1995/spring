<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING/DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<!-- <beans> -->
<beans default-init-method="myInit" default-destroy-method="cleanUp">
<bean id="triangle" class="com.springFifth.Triangle" autowire="byName" scope="singleton" init-method="myInit" destroy-method="cleanUp">

</bean>

<bean id="pointA" class="com.springFifth.Point">
<property name="x" value="0"/>
<property name="y" value="0"/>
</bean>

<bean id="pointB" class="com.springFifth.Point">
<property name="x" value="-20"/>
<property name="y" value="0"/>
</bean>

<bean id="pointC" class="com.springFifth.Point">
<property name="x" value="0"/>
<property name="y" value="20"/>
</bean>


</beans>